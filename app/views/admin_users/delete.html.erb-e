<% @page_title = "Delete" %>


<div class="container">
	<div class="row">
		<div class="col-sm-8">
			<h2>Delete Admin User</h2>

			<%= form_for(:admin_user, :url => {:action => 'destroy', :id => @admin_user.id}) do |f| %>

			<p>Are you sure you want to permanently delete this admin user?</p>

			<p class="reference-name"><%= @admin_user.name %></p>

			<div class="form buttons">
				
				<%= link_to("Cancel", {:action => 'index'}, :class => 'btn btn-default btn-xs') %>
					<%= submit_tag("Delete", :class => 'btn btn-danger btn-xs') %>
			</div>

			<% end %>
		</div>
	</div>
</div>
					
			</div>
		</div>
  

